<a class="bs-tooltip btn btn-default btn-like-replay" href="[% h.get_id(replay) %]" data-toggle="tooltip" data-container="body" data-placement="bottom" title="[% h.loc('button.like.title') %]"><span class="badge">[% replay.site.like || '0' %]</span> [% h.loc('button.like.text') %]</a>
<a class="bs-tooltip btn btn-default btn-save-replay" data-placement="bottom" data-toggle="tooltip" data-container="body" title="[% h.loc('button.save.title') %]" href="[% config.urls.replays %]/[% replay.file %]"><span class="badge">[% replay.site.downloads || '0' %]</span> [% h.loc('button.save.text') %]</a>
<a class="bs-tooltip btn btn-default btn-embed-replay [% IF !replay.site.banner.available %]disabled[% END %]" data-placement="bottom" data-toggle="tooltip" data-container="body" href="[% h.get_id(replay) %]" title="[% h.loc('button.embed.title') %]">[% h.loc('button.embed.text') %]</a>
<a class="bs-tooltip btn btn-default btn-battleviewer [% IF !replay.packets.defined %]disabled[% END %]" data-placement="bottom" data-toggle="tooltip" data-container="body"  title="[% h.loc('button.battleviewer.title') %]" href="/battleviewer/[% h.get_id(replay) %].html">[% h.loc('button.battleviewer.text') %]</a>
<a class="bs-tooltip btn btn-default btn-battleheatmap [% IF !replay.heatmap.defined %]disabled[% END %]" data-placement="bottom" data-toggle="tooltip" data-container="body"  title="[% h.loc('button.battleheatmap.title') %]" href="/battleheatmap/[% h.get_id(replay) %].html">[% h.loc('button.battleheatmap.text') %]</a>
