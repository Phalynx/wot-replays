[% WRAPPER wrapper.html.tt %]
    <script src="/js/mapgrid.js" type="text/javascript"></script>
    <script src="/js/heatmap.js" type="text/javascript"></script>
    <div class="page-header">
        <h2>Test Page</h2>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12">
            <div id="test-container"></div>
        </div>
    </div>
    <script type="text/javascript">
        var s = new EventSource('http://api.wotreplays.org/v1/replay/52b162f59c81a506e2010000/packets.evt');

        s.onmessage = function(evt) {
            console.log('msg: ', evt);
        });

        s.addEventListener('packetstart', function(evt) {
            $('#test-container').append($('<div/>').text('start'));
        });
        s.addEventListener('packetdone', function(evt) {
            $('#test-container').append($('<div/>').text('stop'));
            s.close();
        });

        s.addEventListener('packet', function(evt) {
            $('#test-container').append($('<div/>').append($('<pre/>').text(evt.data)));
        });
    </script>
[% END %]
